// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:123456@localhost:5433/data1"
}

model Attendance {
  id           String    @id @map("id")
  time         DateTime  @map("time")
  date         DateTime  @map("date")
  time2        DateTime  @map("time2")
  fname        String    @map("fname")
  lname        String    @map("lname")
  name         String    @map("name")
  rname        String    @map("rname")
  group        String    @map("group")
  card_number  String    @map("card_number")
  pic          String?   @map("pic")
  dev          String    @map("dev")
  
  employee     Employee  @relation(fields: [id], references: [id])
  
  @@map("table3")
}

model Employee {
  id          String       @id @map("id")
  first_name  String    @map("first_name")
  last_name   String    @map("last_name")
  department  String    @map("department")
  shift       String?   @map("shift")
  
  attendances Attendance[]
  
  @@map("details")
}
